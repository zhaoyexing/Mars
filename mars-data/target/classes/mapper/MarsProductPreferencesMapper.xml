<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.st.mapper.MarsProductPreferencesMapper">
    <select id="getCharacterName" parameterType="java.lang.Integer" resultType="java.lang.String">
        SELECT className AS className FROM
        mars_tag_class
        WHERE pid = #{id}
    </select>

</mapper>